<!-- all card edit stuff-->
<div class="card-edit-view-parent"></div>

<script type="text/javascript">

  $(function(){
    getCardEdit('.card-edit-view-parent', <%= @card.to_json(:include => { :style => {:only => :template_name },:image => {:only => :url },
      :background => {:only => :url },

      }).to_s.html_safe %>,
      <%= @nextCardIndex %>,<%= @prevCardIndex %>,
      <%= @backgrounds.to_json.to_s.html_safe %>,
      <%= @images.to_json.to_s.html_safe %>);
});

</script>

